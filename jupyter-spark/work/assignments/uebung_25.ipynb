    "library(SparkR)\n",
    "\n",
    "# Spark session & context\n",
    "sc <- sparkR.session(\"local\")\n",
    "\n",
    "# Sum of the first 100 whole numbers\n",
    "sdf <- createDataFrame(list(1:100))\n",
    "dapplyCollect(sdf,\n",
    "              function(x)\n",
    "              { x <- sum(x)}\n",
    "             )\n",
    "# 5050"
